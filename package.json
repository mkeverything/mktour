{
  "name": "mktour",
  "version": "0.9.0",
  "private": true,
  "packageManager": "bun@1.3.5",
  "scripts": {
    "dev": "bunx next dev",
    "dev:test": "MKTOURTEST=true bunx next dev",
    "dev:local": "OFFLINE=true bunx next dev",
    "build": "next build",
    "start": "bunx --bun next start",
    "lint": "bunx eslint .",
    "opt": "OPT=true bunx --bun next dev",
    "analyze": "ANALYZE=true next build",
    "format": "bunx --bun prettier . --write",
    "db:push": "drizzle-kit generate && bun run src/server/db/migrate.ts",
    "db:drop": "drizzle-kit drop",
    "db:studio": "drizzle-kit studio",
    "db:check": "drizzle-kit check",
    "localdb:push": "OFFLINE=true drizzle-kit generate && OFFLINE=true bun run src/server/db/migrate.ts",
    "localdb:drop": "OFFLINE=true drizzle-kit drop",
    "localdb:studio": "OFFLINE=true drizzle-kit studio",
    "localdb:check": "OFFLINE=true drizzle-kit check",
    "knip": "knip-bun",
    "generate-erd": "bun run src/lib/generate-erd.ts",
    "prepare": "husky",
    "generate-openapi": "bun run src/server/api/generateOpenApi.ts > src/app/api/spec/openapi-spec.json",
    "test:noseed": "SKIP_SEED=1 bun test",
    "check": "bunx tsc --noEmit && bun lint && bun test"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@libsql/client": "^0.17.0",
    "@lucia-auth/adapter-drizzle": "^1.1.0",
