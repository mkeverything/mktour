    "loglayer": "^9.1.0",
    "lucia": "^3.2.2",
    "lucide-react": "^0.575.0",
    "nanoid": "^5.1.6",
    "next": "^16.1.6",
    "next-intl": "^4.8.3",
    "next-pwa": "^5.6.0",
    "next-themes": "^0.4.6",
    "postcss": "8.5.6",
    "prettier": "^3.8.1",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "react": "^19.2.4",
